{
  "name": "campaign",
  "plural": "campaigns",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string"
    },
    "CPI": {
      "type": "string"
    },
    "CPC": {
      "type": "string"
    },
    "expiration_date": {
      "type": "date",
      "required": true
    },
    "start": {
      "type": "date",
      "required": true
    },
    "duration": {
      "type": "number"
    },
    "partner_id": {
      "type": "number"
    },
    "target": {
      "type": "number"
    },
    "completed": {
      "type": "number"
    },
    "status": {
      "type": "string",
      "default": "pending"
    },
    "view_impressions": {
      "type": "boolean",
      "default": false
    },
    "created_at": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "impressions": {
      "type": "hasMany",
      "model": "impression",
      "foreignKey": "campaign_id"
    },
    "clicks": {
      "type": "hasMany",
      "model": "click",
      "foreignKey": "campaign_id"
    },
    "aDs": {
      "type": "hasMany",
      "model": "AD",
      "foreignKey": "campaign_id",
      "through": "campaign_ad"
    },
    "criterias": {
      "type": "hasMany",
      "model": "criteria",
      "foreignKey": "campaign_id"
    }
  },
  "acls": [],
  "methods": {
    "states": {
      "accepts": [
        {
          "arg": "req",
          "type": "object",
          "http": {
            "source": "req"
          }
        },
        {
          "arg": "partner_id",
          "type": "number",
          "required": false,
          "description": "",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "isActive",
          "type": "boolean",
          "required": true,
          "description": "",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": [
        {
          "arg": "states",
          "type": "array",
          "root": true,
          "description": "return campaigns statestic"
        }
      ],
      "description": "get campaigns statestic",
      "http": [
        {
          "path": "/states",
          "verb": "get"
        }
      ]
    }
  }
}
