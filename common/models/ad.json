{
  "name": "AD",
  "plural": "ADs",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Counts": true
  },
  "properties": {
    "id": {
      "type": "number"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string"
    },
    "media_link": {
      "type": "string"
    },
    "ad_text": {
      "type": "string"
    },
    "thumb_link": {
      "type": "string"
    },
    "status": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "partner_id": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "portrait_link": {
      "type": "string"
    },
    "portrait_thumb_link": {
      "type": "string"
    },
    "botton_title": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "impressions": {
      "type": "hasMany",
      "model": "impression",
      "foreignKey": "ad_id"
    },
    "clicks": {
      "type": "hasMany",
      "model": "click",
      "foreignKey": "ad_id"
    },
    "campaigns": {
      "type": "hasMany",
      "model": "campaign",
      "foreignKey": "ad_id",
      "through": "campaign_ad"
    }
  },
  "acls": [],
  "methods": {
    "randomAD": {
      "accepts": [
        {
          "arg": "limit",
          "type": "string",
          "required": true,
          "description": "",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "client_id",
          "type": "number",
          "required": false,
          "description": "",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "mobile",
          "type": "string",
          "required": false,
          "description": "",
          "http": {
            "source": "query"
          }
        },
        {
          "arg": "location_id",
          "type": "number",
          "required": false,
          "description": "",
          "http": {
            "source": "query"
          }
        }
      ],
      "returns": [
        {
          "arg": "ADs",
          "type": "array",
          "root": true,
          "description": "return random ad"
        }
      ],
      "description": "get random ad",
      "http": [
        {
          "path": "/RandomAD",
          "verb": "get"
        }
      ]
    }
  }
}
