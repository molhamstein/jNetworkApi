      "type": "belongsTo",
      "model": "locations",
      "foreignKey": "location_id"
    },
    "seller": {
      "type": "belongsTo",
      "model": "seller",
      "foreignKey": "seller_id"
    }
  },
  "acls": [],
  "methods": {}
}
